# BUCLES: Conocemos el numero de repeticiones

def bucle_1():
    for cont in range(1,10):
     print(cont)


bucle_1()



# BUCLES: Conocemos el numero de repeticiones

def bucle_1():
    suma=0 #es una variable acumuladora
    for cont in range(1,11):
     num=int(input("Dime un numero: "))
     suma=num+suma
    print(suma)


bucle_1()
